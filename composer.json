{
  "name": "sunnysideup/demo",
  "description": "Sunny Side Up Module Demo",
  "license": "proprietary",
  "authors": [
    {
      "name": "Sunny Side Up",
      "homepage": "http://www.sunnysideup.co.nz",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "silverstripe/recipe-cms": "^4@stable",
    "bimthebam/silverstripe-native-color-input": "^1.0",
    "sunnysideup/error-log-made-easy": "dev-master",
    "sunnysideup/permission_provider": "dev-master",
    "sunnysideup/webpack_requirements_backend": "dev-master",
    "sunnysideup/silverstripe-externalurlfield": "dev-master",
    "sunnysideup/sentry-made-easy": "dev-master",
    "sunnysideup/phone_field": "dev-master",
    "sunnysideup/metatags": "dev-master",
    "sunnysideup/email_address_database_field": "dev-master",
    "sunnysideup/title_dataobject": "*@dev",
    "sunnysideup/simple-template-caching": "dev-master",
    "silverstripe/totp-authenticator": "^4.5",
    "silverstripe/webauthn-authenticator": "^4.6",
    "symfony/event-dispatcher-contracts": "v3.2.0",
    "silverstripe/mfa": "^4.0",
    "sandervanscheepen/silverstripe-cms-userswitcher": "dev-master",
    "sunnysideup/default-predict": "dev-main",
    "silverstripe/userforms": "^5.14",
    "symfony/event-dispatcher": "^5.4",
    "sunnysideup/yes-no-any-filter": "dev-master",
    "sunnysideup/flush-front-end": "dev-master",
    "sunnysideup/export-all-from-modeladmin": "dev-master",
    "sunnysideup/cms-dark-theme": "dev-main"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "^3.2.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Sunnysideup\\Demo\\": "app/src/"
    },
    "files": [
      "app/src/Page.php",
      "app/src/PageController.php"
    ]
  },
  "files-to-add": [
    "app/src/Page.php",
    "app/src/PageController.php"
  ],
  "extra": {
    "expose": [
      "app/client"
    ],
    "resources-dir": "_resources",
    "project-files-installed": [
      "app/.htaccess",
      "app/_config.php",
      "app/_config/database.yml",
      "app/_config/mimevalidator.yml",
      "app/_config/mysite.yml",
      "app/src/Page.php",
      "app/src/PageController.php"
    ],
    "public-files-installed": [
      ".htaccess",
      "index.php",
      "web.config"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true,
      "silverstripe/recipe-plugin": true,
      "php-http/discovery": true
    }
  },
  "conflict": {}
}
